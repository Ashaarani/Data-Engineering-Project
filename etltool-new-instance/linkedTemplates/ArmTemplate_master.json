{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "etltool-new-instance"
		},
		"SourceAzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'SourceAzureDataLakeStorage'"
		},
		"Source_linked_services_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Source_linked_services'"
		},
		"destAzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'destAzureDataLakeStorage'"
		},
		"dest_linked_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'dest_linked_service'"
		},
		"SourceAzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://newstorageproject.dfs.core.windows.net/"
		},
		"Source_linked_services_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://newstorageproject.dfs.core.windows.net/"
		},
		"destAzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://deststorageproject.dfs.core.windows.net/"
		},
		"dest_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://deststorageproject.dfs.core.windows.net/"
		},
		"newtrig_properties_pipeline1_copy1_parameters_file_name": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"newtrig_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/93d189b4-0cd2-415b-822d-c0a9607e9150/resourceGroups/Data_Engineering_project/providers/Microsoft.Storage/storageAccounts/newstorageproject"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "etltool-new-instance_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SourceAzureDataLakeStorage_accountKey": {
						"value": "[parameters('SourceAzureDataLakeStorage_accountKey')]"
					},
					"Source_linked_services_accountKey": {
						"value": "[parameters('Source_linked_services_accountKey')]"
					},
					"destAzureDataLakeStorage_accountKey": {
						"value": "[parameters('destAzureDataLakeStorage_accountKey')]"
					},
					"dest_linked_service_accountKey": {
						"value": "[parameters('dest_linked_service_accountKey')]"
					},
					"SourceAzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('SourceAzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"Source_linked_services_properties_typeProperties_url": {
						"value": "[parameters('Source_linked_services_properties_typeProperties_url')]"
					},
					"destAzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('destAzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"dest_linked_service_properties_typeProperties_url": {
						"value": "[parameters('dest_linked_service_properties_typeProperties_url')]"
					},
					"newtrig_properties_pipeline1_copy1_parameters_file_name": {
						"value": "[parameters('newtrig_properties_pipeline1_copy1_parameters_file_name')]"
					},
					"newtrig_properties_typeProperties_scope": {
						"value": "[parameters('newtrig_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}